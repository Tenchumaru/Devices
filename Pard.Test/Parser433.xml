<?xml version="1.0" encoding="utf-8" ?>
<!-- Page 218 -->
<grammar>
  <symbols>
    <nonterminal name="E" type="int" />
    <nonterminal name="T" type="int" />
    <nonterminal name="F" type="int" />
    <terminal name="id" type="int" />
  </symbols>
  <rules>
    <rule name="E">
      <nonterminal name="E"/>
      <literal value="+"/>
      <nonterminal name="T"/>
      <action><![CDATA[$$= Result= $1 + $3;]]></action>
    </rule>
    <rule name="E">
      <nonterminal name="T"/>
      <action><![CDATA[$$= Result= $1;]]></action>
    </rule>
    <rule name="T">
      <nonterminal name="T"/>
      <literal value="*"/>
      <nonterminal name="F"/>
      <action><![CDATA[$$= $1 * $3;]]></action>
    </rule>
    <rule name="T">
      <nonterminal name="F"/>
    </rule>
    <rule name="F">
      <literal value="("/>
      <nonterminal name="E"/>
      <literal value=")"/>
      <action><![CDATA[$$= $2;]]></action>
    </rule>
    <rule name="F">
      <terminal name="id"/>
    </rule>
  </rules>
</grammar>